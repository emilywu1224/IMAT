<!--
	@file This is the generic ant script for copying resources. 
	@author rpfeffer 
	@dateCreated Dec 2, 2011
  -->
<project default="execute" >
	<!-- 
	Move a resource from the ${src} location to the ${target} location. 
	-->
	<target name="execute" depends="move" />

	<target name="move">
		<move file="${src}" tofile="${target}"/>
	</target>
</project>